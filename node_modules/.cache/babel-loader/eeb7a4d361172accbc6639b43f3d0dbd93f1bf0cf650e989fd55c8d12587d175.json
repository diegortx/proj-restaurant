           <button\n              class=\"menu-1-1-3-sum\"\n              @click=\"bebida.count++ | sum(bebida.price)\"\n            >\n              +\n            </button>\n            <div class=\"menu-1-1-3-num\">{{ bebida.count }}</div>\n            <button\n              class=\"menu-1-1-3-sub\"\n              :disabled=\"bebida.count == 0\"\n              @click=\"bebida.count-- | sub(bebida.price)\"\n            >\n              -\n            </button>\n          </div>\n        </div>\n        <hr />\n      </div>\n      <div class=\"menu-footer\">\n        <div class=\"table\">Mesa {{ table.id }}</div>\n        <div class=\"account\">Total: R$ {{ value }}</div>\n      </div>\n\n      <div class=\"buttom\">\n        <button class=\"cancel\" @click=\"toHome\">Cancelar</button>\n        <button class=\"apply\" @click=\"apply\">Confirmar</button>\n      </div>\n    </div>\n  </div>\n</template>\n    \n<script>\nexport default {\n  name: \"OrderScreen\",\n  props: {},\n  components: {},\n  data() {\n    return {\n      table: [],\n      value: 0,\n    };\n  },\n\n  mounted() {\n    this.table = this.$store.state.tables.find(\n      (table) => table.id == this.$route.params.id\n    );\n    this.value = this.table.value;\n  },\n  methods: {\n    toHome() {\n      this.$router.push({\n        name: \"Home\",\n      });\n    },\n    hour() {\n      const date = new Date();\n      const hour = date.getHours() + \":\" + date.getMinutes() + \":\" + date.getSeconds();\n      return hour;\n    },\n    apply() {\n      this.$store.commit(\"setCount\", {\n        id: this.$route.params.id,\n        value: this.value,\n        hour: new Date(),\n      });\n\n      this.toHome();\n    },\n    sum(price) {\n      this.value = this.value + price;\n    },\n    sub(price) {\n      this.value = this.value - price;\n    },\n  },\n};\n</script>\n    \n  <style scoped>\n.main {\n  margin-top: 80px;\n  width: 60vw;\n  flex-wrap: wrap;\n  display: flex;\n  justify-content: center;\